<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>{% block title %}Welcome!
        {% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Gestion des rendez-vous médicaux" name="HASSAINE YOUSRA CHAHDI IKRAM" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="{{ asset('Admin/assets/img/favicon.png') }}">

    <!-- plugin css -->
    <link href="{{ asset('Admin/assets/libs/jsvectormap/css/jsvectormap.min.css') }}" rel="stylesheet" type="text/css" />

    <!-- swiper css -->
    <link rel="stylesheet" href="{{ asset('Admin/assets/libs/swiper/swiper-bundle.min.css') }}">
    <!-- Sweet Alert-->
    <link href="{{ asset('Admin/assets/libs/sweetalert2/sweetalert2.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- Bootstrap Css -->
    <link href="{{ asset('Admin/assets/css/bootstrap.min.css') }}" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="{{ asset('Admin/assets/css/icons.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="{{ asset('Admin/assets/css/app.min.css') }}" id="app-style" rel="stylesheet" type="text/css" />

    {% block stylesheets %}

        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body data-topbar="dark">

<!-- Begin page -->
<div id="layout-wrapper">

    <header id="page-topbar">
        <div class="navbar-header">
            <div class="d-flex">
                <!-- LOGO -->
                <div class="navbar-brand-box">
                    <a href="index.html" class="logo logo-dark">
                        <span class="logo-sm">
                            <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26">
                        </span>
                        <span class="logo-lg">
                            <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26"> <span class="logo-txt">Syndic PFA</span>
                        </span>
                    </a>

                    <a href="index.html" class="logo logo-light">
                        <span class="logo-sm">
                            <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26">
                        </span>
                        <span class="logo-lg">
                            <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26"> <span class="logo-txt">Syndic PFA</span>
                        </span>
                    </a>
                </div>

                <button type="button" class="btn btn-sm px-3 header-item vertical-menu-btn noti-icon">
                    <i class="fa fa-fw fa-bars font-size-16"></i>
                </button>

            </div>
            <div class="d-flex">
                <div class="dropdown d-inline-block d-block d-lg-none">
                    <button type="button" class="btn header-item noti-icon"
                            data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="bx bx-search icon-sm"></i>
                    </button>
                </div>
                <div class="dropdown d-inline-block">
                    <button type="button" class="btn header-item user text-start d-flex align-items-center" id="page-header-user-dropdown"
                            data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <img class="rounded-circle header-profile-user" src="{{ asset('Admin/assets/images/users/avatar-1.jpg') }}"
                        alt="Header Avatar">
                        <span class="ms-2 d-none d-xl-inline-block user-item-desc">
                                <span class="user-name"> </span>
                            </span>
                    </button>
                </div>
            </div>
        </div>
    </header>

    <div class="vertical-menu">


        <!-- LOGO -->
        <div class="navbar-brand-box">
            <a href="index.html" class="logo logo-dark">
                    <span class="logo-sm">
                        <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26">
                    </span>
                <span class="logo-lg">
                        <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26"> <span class="logo-txt">Syndic</span>
                    </span>
            </a>

            <a href="index.html" class="logo logo-light">
                    <span class="logo-sm">
                        <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26">
                    </span>
                <span class="logo-lg">
                        <img src="{{ asset('Admin/assets/images/logo-sm.svg') }}" alt="" height="26"> <span class="logo-txt">Syndic</span>
                    </span>
            </a>
        </div>

        <button type="button" class="btn btn-sm px-3 font-size-16 header-item vertical-menu-btn">
            <i class="fa fa-fw fa-bars"></i>
        </button>

        <div data-simplebar class="sidebar-menu-scroll">

            <!--- Sidemenu -->
            <div id="sidebar-menu">
                <!-- Left Menu Start -->
                <ul class="metismenu list-unstyled" id="side-menu">
                    <li class="menu-title" data-key="t-menu">Menu</li>

                    <li class="menu-title" data-key="t-applications">Applications</li>

                    <li>
                        <a >
                            <i class="bx bx-buildings nav-icon"></i>
                            <span class="menu-item" data-key="t-calendar">Copropriete</span>
                        </a>
                    </li>

                </ul>
            </div>

        </div>

    </div>
    <div class="main-content">
        <div class="page-content">
            <div class="container-fluid">
                <!--Add Row-->
                {% block body %}
                {% endblock %}


            </div>

        </div>
        <footer class="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6">
                        <script>document.write(new Date().getFullYear())</script> &copy; Syndic.
                    </div>
                    <div class="col-sm-6">
                        <div class="text-sm-end d-none d-sm-block">
                            réalisé par <i class="mdi mdi-heart text-danger"></i> FRIKAL Ahmed Mimouni Achraf,Belmir Ahmed
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

</div>



<script src="{{ asset('Admin/assets/libs/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
<script src="{{ asset('Admin/assets/libs/metismenujs/metismenujs.min.js') }}"></script>
<script src="{{ asset('Admin/assets/libs/simplebar/simplebar.min.js') }}"></script>
<script src="{{ asset('Admin/assets/libs/feather-icons/feather.min.js') }}"></script>

<!-- swiper js -->
<script src="{{ asset('Admin/assets/libs/swiper/swiper-bundle.min.js') }}"></script>
<script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>

<!-- Sweet Alerts js -->
<script src="{{ asset('Admin/assets/libs/sweetalert2/sweetalert2.min.js') }}"></script>


<script src="https://cdnjs.cloudflare.com/ajax/libs/dropzone/5.9.3/min/dropzone.min.js"></script>

<script src="{{ asset('Admin/assets/js/app.js') }}"></script>


{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>

</html>
